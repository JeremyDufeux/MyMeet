<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeremydufeux.mymeet">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@drawable/ic_my_meet"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_my_meet"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyMeet">
        <activity android:name=".ui.add.AddMeetingActivity"/>
        <activity android:name=".ui.list.ListMeetingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>